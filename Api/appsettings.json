{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "CONNECTION_STRING"
  },
  "FirestoreStorage": {
    "Default": "STORAGE_LINK",
    "DefaultBucket": "BUCKET_NAME"
  },

  "YouTubeApi": {
    "ApiKey": "YOUTUBE_KEY"
  },
  "Auth": {
    "TokenUrl": "https://identitytoolkit.googleapis.com/v1/accounts:signInWithPassword?key=FirebaseKey",
    "RefreshTokenUrl": "https://securetoken.googleapis.com/v1/token?key=FirebaseKey",
    "Key": "FirebaseKey",
    "ValidIssuer": "https://securetoken.google.com/project-id",
    "Audience": "project-id"
  },
  "AllowedHosts": "*"
}
