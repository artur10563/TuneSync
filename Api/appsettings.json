{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "${DATABASE_CONNECTION_STRING}"
  },
  "FirestoreStorage": {
    "Default": "gs://${STORAGE_LINK}",
    "DefaultBucket": "${BUCKET_NAME}"
  },
  "FirebaseConfig": {
    "type": "service_account",
    "project_id": "${PROJECT_ID}",
    "private_key_id": "${PRIVATE_KEY_ID}",
    "private_key": "-----BEGIN PRIVATE KEY-----${Firebase_SDK_PRIVATE_KEY}\n-----END PRIVATE KEY-----\n",
    "client_email": "${FIREBASE_CLIENT_EMAIL}",
    "client_id": "${CLIEN_ID",
    "auth_uri": "${AUTH_URI}",
    "token_uri": "${TOKEN_URL}",
    "auth_provider_x509_cert_url": "{$AUTH_PROVIDER}",
    "client_x509_cert_url": "${ADMIN_SERVICE_ACC}",
    "universe_domain": "googleapis.com"
  },
  "YouTubeApi": {
    "ApiKey": "{YOUTUBE_KEY}"
  },
  "Auth": {
    "TokenUrl": "https://identitytoolkit.googleapis.com/v1/accounts:signInWithPassword?key=${FIREBASE_KEY}",
    "RefreshTokenUrl": "https://securetoken.googleapis.com/v1/token?key=${FIREBASE_KEY}",
    "Key": "${FIREBASE_KEY}",
    "ValidIssuer": "https://securetoken.google.com/${PROJECT_ID}",
    "Audience": "${PROJECT_ID}"
  },
  "YouTubeCookies": {
    "YSC": "your_ysc_cookie",
    "PREF": "your_pref_cookie",
    "VISITOR_INFO1_LIVE": "your_visitor_info_cookie",
    "SID": "your_sid_cookie",
    "HSID": "your_hsid_cookie",
    "SSID": "your_ssid_cookie",
    "APISID": "your_apisid_cookie",
    "SAPISID": "your_sapisid_cookie",
    "LOGIN_INFO": "your_login_info_cookie"
  },
  "AllowedHosts": "*"
}
