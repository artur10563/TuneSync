{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "CONNECTION_STRING"
  },
  "FirestoreStorage": {
    "Default": "STORAGE_LINK",
    "DefaultBucket": "BUCKET_NAME"
  },

  "YouTubeApi": {
    "ApiKey": "YOUTUBE_KEY"
  },
  "Auth": {
    "TokenUrl": "https://identitytoolkit.googleapis.com/v1/accounts:signInWithPassword?key=[API_KEY]",
    "ValidIssuer": "project-id",
    "Audience": "https://securetoken.google.com/project-id"
  },
  "AllowedHosts": "*"
}
